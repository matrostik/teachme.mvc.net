@{
    ViewBag.Title = "אודות";
}
<div class="breadcrumbs">
    <div class="container">
        <h2 class="pull-right"><i class="fa fa-info-circle color-green"></i> &nbsp;@ViewBag.Title</h2>
    </div><!--/container-->
</div>
